        "# numpyの読み込み\n",
        "import numpy as np\n",
        "\n",
        "#AとBの行列を定義\n",
        "arrayA = np.array([[0, 1, ], [2, 3,], [4, 5]])\n",
        "arrayB = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "\n",
        "#計算結果の表示\n",
        "seki = np.dot(arrayA,arrayB)\n",
        "print(seki)\n",
        "\n",
        "#行列の最大値を求める\n",
        "seki.max()"
      ]
